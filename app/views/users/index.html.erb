<h3>Users (<%= @users.size %>)</h3>
<hr>
<table class="table">
	<thead>
		<th>ID</th>
		<th>Email</th>
		<th>Created</th>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.email %></td>
			<td><%= user.created_at %></td>
		</tr>
		<% end %>
	</tbody>
</table>